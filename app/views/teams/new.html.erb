<div class="container ptb">
  <div class="row">
    
    <div class="col-md-12">
      
      <%= form_for(@team) do |f| %>
        <%= f.hidden_field :admin, :value => current_user.id %>

        <div class="field">
          <%= f.label :name %><br>
          <%= f.text_field :name %>
        </div>
        <div class="field">
          <%= f.label :repo %><br>
          <%= f.text_field :repo %>
        </div>
        <div class="field">
          <%= f.label :video %><br>
          <%= f.text_field :video %>
        </div>
        <div class="actions">
          <%= f.submit %>
        </div>
      <% end %>


    </div>

  </div>
</div>